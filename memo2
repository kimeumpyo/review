9. 비구조화 할당
		const person = {
			name : "이름",
			age : 40
		}

		const { name : name, age : age} = person;

		//

		const person = {
			name : "이름",
			age : 40
		}

		const {name : name2, age} = person;
		
		name2
		=>

		//

		const name = "이름:
		const age = 40;
		const obj1 = { name : name, age : age};
		obj1;
		=>

		const obj2 = {name, age};
		name;
		=>
		age;
		=>

		//

		const fruits = ['apple', 'orange', 'mango'];

		const [f1, f2, f3] = fruits;

		f1;
		=> 'apple'
		f2;
		=>
		f3;
		=>

		//

		let a = 1, b = 2;

		let [b,a] = [a,b];
		
		b;
		=> 1

		a;
		=> 2

		//

		
		let a = 1, b = 2;

		let tmp

		tmp = a;

		a = b;

		b = tmp;








	

